{
    "manifest_version": 2,

    "name": "Bookmark Search",
    "description": "This extension allows you to search your bookmarks",
    "version": "1.0",

    "omnibox": { "keyword" : "%" },
    
    "browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
    },
    "background": {
	"scripts":[ "background.js" ]
    },
    "permissions": [
	"bookmarks"
    ]
}
